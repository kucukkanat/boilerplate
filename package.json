{
  "name": "Boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
	"start":"screwy",
	"start-dev-env": "npm-run-all --parallel dev:*",
	"predev:js": "notify -t Started -m 'Development server started' -i assets/images/franx_logo.png",
    "dev:js": "webpack-dev-server",
    "dev:sass": "node-sass src/styles/app.scss dist/app.css --output-style compressed --source-map dist/app.css.map -w",
    "build": "npm-run-all build:*",
	"build:js": "webpack",
	"postbuild:js": "notify -t 'Javascript Build Completed' -m Complete  -i assets/images/franx_logo.png",
	"build:sass": "node-sass src/styles/app.scss dist/app.css --output-style compressed --source-map dist/app.css.map",
	"postbuild:sass": "notify -t 'Sass Build Completed' -m Complete  -i assets/images/franx_logo.png",
	"build:webfont": "webfont assets/**/*.svg -t css -d dist -f franx-icon --normalize",
	"postbuild:webfont": "notify -t 'Webfonts Build Completed' -m 'SVG files compiled into webfont in /dist folder' -i assets/images/franx_logo.png",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix src",
    "jsdoc": "jsdoc src/**/*.js -d dist",
    "screwy": "screwy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "jsdoc": "^3.5.4",
    "material-ui": "^0.19.0",
    "node-notifier-cli": "^1.0.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "screwy": "^1.0.5",
    "webfont": "^7.1.4",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
